citHeader("To cite ikhyd in publications use:")

citEntry(entry = "Manual",
  title        = "ikhyd: I Know How You Drive - Collection of R functions to make telematics analysis easier.",
  author       = personList(as.person("Issac Lee")),
  year         = "2020",
  note         = "R package version 0.1.0",
  url          = "https://github.com/issactoast/ikhyd",

  textVersion  =
  paste("Issac Lee (2020).",
        "ikhyd: I Know How You Drive - Collection of R functions to make telematics analysis easier.",
        "R package version 0.1.0.",
        "URL https://github.com/issactoast/ikhyd")
)